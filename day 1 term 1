#include<stdio.h>
main()
{
	float a, b, c,t;
	printf("plz input a,b,c:\n");
	scanf("%f%f%f", &a, &b, &c);
	if (a > b)
	{
		t = a; a = b; b = t;//交换ab的值使a<b
	}
	if (a > c)//same dude
	{
		t = a; a = c; c = t;
	}

	if (b > c)// I can handle twice, no thrice
	{
		t = b; b = c; c = t;
	}
		printf("%f,%f,%f", a, b, c);
	return 0;
}
