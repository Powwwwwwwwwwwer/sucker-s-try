#include<stdio.h>
main()
{
	float a, b, c,t;
	printf("plz input a,b,c:\n");
	scanf("%f%f%f", &a, &b, &c);
	if (a > b)
	{
		t = a; a = b; b = t;
	}
	if (a > c)
	{
		t = a; a = c; c = t;
	}

	if (b > c)
	{
		t = b; b = c; c = t;
	}
		printf("%f,%f,%f", a, b, c);
	return 0;
}
//直接按abc的顺序输出但是通过if语句通过中间值t将值互换使a<b<c
